{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "complexity-report": "./node_modules/.bin/es6-plato -r -d ./report src",
    "test": "npx cypress open",
    "test:ci": "npx cypress run --spec **/*.features",
    "test:tags": "npx cypress-tags run -e TAGS=$TAGS"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "tests/e2e/specs",
    "commonPath": "tests/e2e/common",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "tests/e2e/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0",
    "core-js": "^2.6.5",
    "express": "^4.17.1",
    "socket.io-client": "^2.2.0",
    "uws": "^100.0.1",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-google-charts": "^0.3.2",
    "vue-headful": "^2.0.1",
    "vue-material-design-icons": "^4.0.1",
    "vue-mc": "^0.6.0",
    "vue-router": "^3.1.3",
    "vue-socket.io": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^9.0.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "es6-plato": "^1.2.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
